vtsst_result=[];
vtsst_keyref=0;
vtsst_pd=0;
vtsst_tr=0;
vtsst_last=0;
vtsst_e=0;
vtsst_nnum=0;
vtsst_errmat=[0 0 0];
vtsst_tb=0;
vtsst_wwhite=0;
vtsst_kref=0; %%%%%%%%%%%%%%%%%%%%%%%
vtsst_d=1;
vtsst_n='';
vtsst_nd=2;
vtsst_valini=200;
vtsst_valinitext=num2str(vtsst_valini);
vtsst_digres=7;
vtsst_digrestext=num2str(vtsst_digres);
vtsst_temptest=15;
vtsst_temptestex=num2str(vtsst_temptest);%%%%%%%%%%%%%%%%
